{
  "name": "name",
  "description": "desc",
  "author": "me",
  "private": true,
  "scripts": {
      "serve": "http-server dist -s -c 1 -a localhost",
      "local": "npm-run-all --parallel watchify serve gatekeeper:local",
      "build:css": "cat src/css/app.css src/editors/cledit/cledit.css node_modules/bootstrap/dist/css/bootstrap.min.css node_modules/bootstrap/dist/css/bootstrap-theme.min.css node_modules/font-awesome/css/font-awesome.min.css > dist/app.min.css",
      "build:js": "cross-env NODE_ENV=production browserify -g envify src/main.js | uglifyjs -c warnings=false -m > dist/build.js",
      "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o dist/build.js",
      "dev": "npm-run-all --parallel build:js build:css gatekeeper:dev",
      "gatekeeper:local": "node gatekeeper/server.js",
      "gatekeeper:dev": "node gatekeeper/server.js netlify" 
  },
  "dependencies": {
    "bootstrap": "^3.3.5",
    "googlediff": "^0.1.0",
    "clunderscore": "^1.0.0",
    "prismjs": "0.0.1",
    "cledit": "^1.2.0",
    "vue": "^2.0.1",
    "vue-resource": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
      "browserify-hmr": "^0.3.1",
      "brfs": "^1.4.3",
    "cross-env": "^1.0.6",
    "envify": "^3.4.1",
    "font-awesome": "^4.4.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^2.1.2",
    "uglify-js": "^2.5.0",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}
